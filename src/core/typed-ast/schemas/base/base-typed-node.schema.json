{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "base-typed-node",
  "title": "Base Typed Node",
  "description": "基础类型化 AST 节点，所有其他节点的根接口",
  "type": "object",
  "properties": {
    "kind": {
      "description": "AST 节点的类型标识符",
      "type": "integer",
      "minimum": 1,
      "maximum": 357
    },
    "flags": {
      "description": "AST 节点的标志位",
      "type": "integer",
      "minimum": 0
    },
    "parent": {
      "description": "父节点引用",
      "anyOf": [
        {
          "$ref": "common/type-unions#/definitions/AnyTypedNode"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "kind"
  ],
  "additionalProperties": false
}