{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "operator-types",
  "title": "Operator Types",
  "description": "操作符类型定义",
  "definitions": {
    "BinaryOperator": {
      "description": "二元操作符",
      "type": "string",
      "enum": [
        "+",
        "-",
        "*",
        "/",
        "%",
        "**",
        "==",
        "!=",
        "===",
        "!==",
        "<",
        ">",
        "<=",
        ">=",
        "<<",
        ">>",
        ">>>",
        "&",
        "|",
        "^",
        "&&",
        "||",
        "??",
        "=",
        "+=",
        "-=",
        "*=",
        "/=",
        "%=",
        "**=",
        "<<=",
        ">>=",
        ">>>=",
        "&=",
        "|=",
        "^=",
        "&&=",
        "||=",
        "??="
      ]
    },
    "UnaryOperator": {
      "description": "一元操作符",
      "type": "string",
      "enum": [
        "+",
        "-",
        "!",
        "~",
        "++",
        "--",
        "typeof",
        "void",
        "delete"
      ]
    },
    "AssignmentOperator": {
      "description": "赋值操作符",
      "type": "string",
      "enum": [
        "=",
        "+=",
        "-=",
        "*=",
        "/=",
        "%=",
        "**=",
        "<<=",
        ">>=",
        ">>>=",
        "&=",
        "|=",
        "^=",
        "&&=",
        "||=",
        "??="
      ]
    }
  }
}